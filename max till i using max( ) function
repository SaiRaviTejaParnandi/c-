#include<iostream>
#include <algorithm>
using namespace std;
int main()
{   int n;
    cout<<"enter the array size\n";
    cin>>n;
    cout<<"enter array elements\n";
    int a[n];
    for (int i=0;i<n;i++)
    {
    	cin>>a[i];
	}

	int mx=-19999;
	for (int i=0;i<=5;i++)
	{
		mx=max(mx,a[i]);
		cout<<mx<<" ";
	}
return 0;
}
