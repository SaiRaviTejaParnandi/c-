#include<iostream>
#include<string>
using namespace std;
int main()
{
	string name;
	getline(cin,name);
	cout<<"the name you enterd is "<<name<<endl;
	for (int i=0;i<name.size();++i)
	{
		if(name[i]>='a'&& name[i]<='z')
		{
			name[i]=name[i]-32;
		}
	}
	cout<<"after converting string  into uppercase letters"<<name<<endl;
	return 0 ;
}
