#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main()
{   
    int n;
    int max=0;
    cout<<"enter size"<<endl;
    cin>>n;
	int a[n];
	cout<<"enter array elements"<<endl;
	for (int i=0;i<n;++i)
	{
		cin>>a[i];
	}
	for (int i=0;i<n;++i)
	{
		if(a[i]>max)
		{
			max=a[i];
		}
	} 
	cout<<"the maxiumu no is\n"<<max<<endl;
	return 0 ;
}
