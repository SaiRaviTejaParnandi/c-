#include<iostream>
#include <algorithm>
using namespace std;
int main()
{   int n;
    cout<<"enter the array size\n";
    cin>>n;
    cout<<"enter array elements\n";
    int a[n];
    for (int i=0;i<n;i++)
    {
    	cin>>a[i];
	}
    int sum=0;
	for (int i=0;i<n;i++)
	{   
	    sum=0;
	
		for (int j=i;j<n;j++)
		{
			sum=sum+a[j];
			cout<<sum<<" ";
		}
	}
return 0;
}
